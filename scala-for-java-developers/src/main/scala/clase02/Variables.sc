object worksheet {

  // val: Inmutable object
  val result = 7 * 3+1
  // No re-assign is possible
  // result = 7

  // var: Mutable object - re-assign is possible
  var anotherResult = 7+3
  anotherResult = 88

  // the variable is an Int and it can not get assigned another data type
  //anotherResult = "some name"

  // explicit data-type declaration
  var myNumber: Int = 9
}